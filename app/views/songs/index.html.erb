<p>songs</p>

<ul>
<% @songs.each do |song| %>
  <li><%= song.title %></li>
<% end %>
</ul>

<% if user_signed_in? %>
  <%= link_to 'New Song', new_song_path %>
  <%= link_to 'log out', destroy_user_session_url, method: :delete %>
<% else %>
  <%= link_to 'log in', new_user_session_url %>
<% end %>

